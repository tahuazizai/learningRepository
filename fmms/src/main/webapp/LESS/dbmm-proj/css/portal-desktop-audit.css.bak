.login-bg {
  position: fixed;
  width: 100%;
  min-height: 100%;
  background: url('../img/login-bg.png') no-repeat bottom center #02134b;
  background-size: cover;
  z-index: 1;
}
.login-main {
  position: absolute;
  left: 0;
  right: 0;
  top: 20%;
  z-index: 2;
}
.login-content {
  margin: auto;
  width: 1000px;
}
.login-content .login-left {
  float: left;
}
.login-content .login-left img {
  width: 478px;
  height: 398px;
}
.login-box {
  float: right;
  width: 360px;
  background: rgba(2, 17, 49, 0.8);
  border-radius: 4px;
  position: relative;
  z-index: 6;
}
.login-box .login-box-cont {
  display: none;
  padding: 40px;
}
.login-box .login-box-cont .login-title {
  padding-bottom: 24px;
  color: #ADD3FF;
  letter-spacing: 0.54px;
  line-height: 26px;
  font-weight: 200;
  font-size: 26px;
}
.login-box .login-box-tips {
  margin: 16px 0;
}
.login-box .login-box-control {
  margin-top: 8px;
}
.login-box .login-box-control.forget-or-remember-pass {
  color: rgba(255, 255, 255, 0.6);
  margin: 16px 0;
  font-size: 14px;
}
.login-box .login-box-control.forget-or-remember-pass input[type="checkbox"] {
  display: none;
}
.login-box .login-box-control.forget-or-remember-pass input[type="checkbox"] + .checkbox-bg {
  background: url(../img/default@2x.png) no-repeat left top;
  background-size: 100% 100%;
  width: 14px;
  height: 14px;
  display: inline-block;
  vertical-align: middle;
}
.login-box .login-box-control.forget-or-remember-pass input[type="checkbox"]:checked + .checkbox-bg {
  background: url(../img/choose@2x.png) no-repeat left top;
  background-size: 100% 100%;
}
.login-box .login-box-control .btn-primary {
  background: #1F5EFD;
  border-radius: 5px;
  letter-spacing: 0.57px;
  padding: 10px 16px;
}
.login-box .login-box-control .btn-lg {
  font-size: 14px;
}
.login-box .login-box-control .link,
.login-box .login-box-control a.link {
  color: rgba(255, 255, 255, 0.6);
}
.login-box .login-box-control .link:hover,
.login-box .login-box-control a.link:hover {
  color: #ffffff;
}
.login-box .login-box-control .link:active,
.login-box .login-box-control a.link:active {
  color: #e6e6e6;
}
.login-form .form-item {
  width: 100%;
  background-color: #fff;
  border-radius: 5px;
  margin-bottom: 16px;
}
.login-form .form-item:last-child {
  margin-bottom: 0;
}
.login-form .form-control {
  width: 100%;
  height: 40px;
  border: none;
  background: none;
  color: #575966;
  font-size: 12px;
}
.login-form .form-control:focus {
  box-shadow: none;
}
.login-form .input-group-addon {
  border: none;
  font-size: 16px;
  padding: 0 16px;
  color: #98a6b3;
}
.login-form .code-addon,
.login-form .code-addon img {
  width: 64px;
  height: 28px;
}
.login-form .form-control {
  font-size: 14px;
  padding-left: 20px;
  padding-right: 20px;
}
.login-form.login-form-group {
  border: 1px solid #d4d5d6;
  border-radius: 2px;
  margin-bottom: 16px;
}
.login-form.login-form-group:last-child {
  margin-bottom: 0;
}
.login-form.login-form-group .form-item {
  border-bottom: 1px solid #d4d5d6;
  margin: 0;
  border-radius: 2px;
}
.login-form.login-form-group .form-item:first-child {
  border-bottom-left-radius: 0;
  border-bottom-right-radius: 0;
}
.login-form.login-form-group .form-item:last-child {
  border-top-left-radius: 0;
  border-top-right-radius: 0;
  border-bottom: none;
}
html,
body {
  position: relative;
  height: 100%;
}
body {
  font-style: normal;
  background: #f2f4f5;
  min-width: 1000px;
}
.animated {
  animation-duration: .5s;
}
.form-horizontal .control-label {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.app-view {
  position: relative;
  min-height: 100%;
}
/* common */
.view-framework-header {
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  z-index: 200;
  background: #3d59b9;
  min-width: 1000px;
  height: 44px;
  border-bottom: none;
  /* 用户区域开始 */
}
.view-framework-header:before,
.view-framework-header:after {
  content: " ";
  display: table;
}
.view-framework-header:after {
  clear: both;
}
.view-framework-header .head-project-name,
.view-framework-header .head-logo,
.view-framework-header .head-user-info,
.view-framework-header .head-nav {
  height: 44px;
}
.view-framework-header .head-project-name {
  line-height: 44px;
  font-size: 18px;
  white-space: nowrap;
  color: #fff;
  padding: 0 16px;
  border-right: 1px solid #334B9C;
  float: left;
}
.view-framework-header .head-logo {
  text-align: center;
  float: left;
}
.view-framework-header .head-logo a {
  padding: 0 8px;
  display: inline-block;
  cursor: default;
}
.view-framework-header .head-logo a,
.view-framework-header .head-logo a img {
  height: 44px;
}
.view-framework-header .head-logo a:hover {
  background-color: 44px;
}
.view-framework-header .head-user-info {
  position: relative;
  float: right;
}
.view-framework-header .head-user-info .info-main {
  position: relative;
  display: block;
  padding: 6px 16px 0 16px;
  height: 44px;
}
.view-framework-header .head-user-info .info-main .user-img {
  position: absolute;
  left: 16px;
  top: 6px;
  z-index: 2;
}
.view-framework-header .head-user-info .info-main .user-img,
.view-framework-header .head-user-info .info-main .user-img img {
  width: 32px;
  height: 32px;
}
.view-framework-header .head-user-info .info-main .user-cont {
  padding-left: 40px;
  font-size: 12px;
  text-align: left;
  line-height: 1;
}
.view-framework-header .head-user-info .info-main .user-cont .user-cont-job {
  font-size: 14px !important;
  color: #fff;
  padding-bottom: 4px;
}
.view-framework-header .head-user-info .info-main .user-cont .user-cont-name {
  color: #fff;
  opacity: .5;
}
.view-framework-header .head-nav {
  position: relative;
}
.view-framework-header .head-nav > ul:before,
.view-framework-header .head-nav > ul:after {
  content: " ";
  display: table;
}
.view-framework-header .head-nav > ul:after {
  clear: both;
}
.view-framework-header .head-nav > ul > li {
  text-align: center;
  float: left;
}
.view-framework-header .head-nav > ul > li > a {
  display: block;
  padding: 0 16px;
  height: 44px;
  line-height: 44px;
  color: #fff;
  font-size: 12px;
}
.view-framework-header .head-nav > ul > li > a.active,
.view-framework-header .head-nav > ul > li > a:hover {
  background-color: #334b9b;
}
.view-framework-header .head-nav > ul > li > a:focus,
.view-framework-header .head-nav > ul > li > a.active {
  color: #fff;
  background-color: #334b9b;
}
.view-framework-header .head-nav > ul > li > a .fa {
  margin-right: 8px;
}
